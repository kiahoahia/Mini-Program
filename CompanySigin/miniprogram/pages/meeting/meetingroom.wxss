.dates{
  width: 100%;
  height: 100rpx;
  border-bottom: 2rpx solid #8888;
  display: flex;
  color: white;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  color: #1380f7;
}

.meeting{
  width:96% ;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-shadow: 2px 2px 2px 2px #8888;
  margin-top: 30rpx;
  border-radius: 10rpx;
  background-color: #fff;
  padding-bottom:30rpx ;
}
.center{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;
}
.meeting_1{
  width: 100%;
  height: 80rpx;
  margin-top: 20rpx;

  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  border-bottom: 2px solid #8888;

}
.room_name{
  font-size: 38rpx;
  
}
.meeting_num{
  width: 25%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
color: #ff9653;
}
.meeting_msg{
  width: 99%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  border-bottom: 2px solid #8888;
  border-left: 8px solid #ff9653;
  color: #697b90;

}
.meeting_time{
  width: 30%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.meeting_department_name{
  width: 70%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.yuyue{
  width: 96%;
  height: 300rpx;
  background-color: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
  box-shadow: 2px 2px 2px 2px #8888;
}
.yuyue_title{
  width: 92%;
  height: 100rpx;
  font-size: 40rpx;
  display: flex;
  align-items: center;
}
.yuyue_start_time{
  width: 92%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2px solid #8888;
}
.yuyue_end_time{
  width: 92%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2px solid #8888;
}
.xuanze{
  color: #697b69;
  width: 40%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}